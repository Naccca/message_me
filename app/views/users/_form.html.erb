<%= render 'shared/errors', obj: @user %>
<%= form_for(@user, html: {class: "ui form"}) do |f| %>
  <div class="field">
    <%= f.label :username %>
    <%= f.text_field :username, placeholder: "Username" %>
  </div>
  <div class="field">
    <%= f.label :password %>
    <%= f.password_field :password, placeholder: "Password" %>
  </div>
  <%= f.submit class: "ui pink button" %>
<% end %>